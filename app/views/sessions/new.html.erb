<div class = "outer">
<div class = "inner">
  <ul class='errors'>
  </ul>
  <div class="form">
  <%= form_for :session, url: sessions_path do |f| %>
      <%= f.email_field :email, placeholder: "Email" %>
      <%= f.password_field :password, placeholder: "Password" %>
      <%= f.submit value: "Log In" %>
  <% end %>
  </div>
  <div>
    Not registered for EARP access? <%= link_to 'Register', new_user_path %>
  </div>
</div>
